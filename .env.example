# Football Training App Environment Variables
# Copy this file to .env and fill in your actual values

# App Configuration
APP_NAME=Football Training Manager
APP_VERSION=1.0.0
ENVIRONMENT=development

# Firebase Configuration
FIREBASE_PROJECT_ID=your-project-id
FIREBASE_API_KEY_ANDROID=your-android-api-key
FIREBASE_API_KEY_IOS=your-ios-api-key
FIREBASE_APP_ID_ANDROID=your-android-app-id
FIREBASE_APP_ID_IOS=your-ios-app-id
FIREBASE_MESSAGING_SENDER_ID=your-sender-id
FIREBASE_STORAGE_BUCKET=your-storage-bucket

# Optional: External Services
EMAIL_SERVICE_API_KEY=your-email-service-key
ANALYTICS_TRACKING_ID=your-analytics-id

# Development Settings
DEBUG_MODE=true
ENABLE_LOGGING=true
MOCK_DATA=false